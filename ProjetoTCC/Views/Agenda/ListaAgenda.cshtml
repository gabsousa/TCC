@model IEnumerable<ProjetoTCC.Models.Agenda>

@{
    ViewData["Title"] = "ListaAgenda";
}

<h2>ListaAgenda</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.cod_agenda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dia_consulta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.hora_consulta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.confirmacao_agenda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.obs_agenda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.resp_agendamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.data_retorno)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cod_agenda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dia_consulta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hora_consulta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.confirmacao_agenda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.obs_agenda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.resp_agendamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.data_retorno)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
